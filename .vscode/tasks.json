{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build AES-DRBG",
            "type": "shell",
            "command": "gcc",
            "args": [
                "aes_drbg.c",
                "-o",
                "aes_drbg.exe",
                "-I",
                "C:/msys64/mingw64/include",
                "-L",
                "C:/msys64/mingw64/lib",
                "-lssl",
                "-lcrypto"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        }
    ]
}

